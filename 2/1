class Student:
  def __init__(self, i, y, u= "МГПУ,ИЦО,АДЭУ-211"):
    self.i=i
    self.u=u
    self.y=y
  def display(self):
    return self.i,self.u,d[self.y]
  def __del__(self):
    if self.y < 3:  #2, 1, 0
      print("Отчислен студент ", self.i, d[self.y])
    else :
      print("Молодцы!")
s1= Student(i="Удзумаки Наруто Намиказович", y=5)
s2= Student(i="Пируш Никита Андреевич", y=2)
d={2:"- Задолжник", 4:"- Ударник",5:"- Отличник"}

print(s1.display())
print(s2.display())
